<!DOCTYPE hibernate-mapping SYSTEM "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="in.app.Model.Employee" table="emptab">
		<id name="empId" type="java.lang.Integer" column="eid" />
		<property name="empName" type="java.lang.String" length="20" column="ename" />
		<property name="empSal" type="java.lang.Double" column="esal" />
	</class>
</hibernate-mapping>